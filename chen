data=pd.read_csv('data.csv',skiprows=0)
# 查看哪些值缺失
nan_all = data.isnull() 
nan_col1 = data.isnull().any() 
nan_col2 = data.isnull().all() 
df2 = data.dropna(thresh=280)
df3=df2.dropna(axis=1,thresh=1200)
nan_result= df3.fillna(method='backfill') 
print (nan_result)
print(type(nan_result))
nan_result.to_csv('test.csv',index=False,sep=',')
